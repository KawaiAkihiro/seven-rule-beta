<h1><%= @event.staff.staff_name %></h1>
<h1><%= @event.fulltime %></h1>

<div class="row">
      <div class="col">
            <%= form_with(model: @event, url: instead_perfect_shift_path(@event), html: {method: :patch},local: false) do |f| %>
